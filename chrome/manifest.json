{
  "author": "Greg Leuch",
  "background": {
    "persistent": true,
    "scripts": [
      "js/jquery-1.11.2.min.js",
      "js/color-thief-2.0.js",
      "js/resample-hermite.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "38": "browseraction38.png"
    },
    "default_popup": "popup.html"
  },
  "description": "Visualize your Internet browsing.",
  "homepage_url": "https://mycolor.today",
  "icons": {
    "128": "icon128.png"
  },
  "manifest_version": 2,
  "name": "MyColor.Today",
  "permissions": [
    "storage",
    "tabs",
    "unlimitedStorage",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "short_name": "MyColor.Today",
  "version": "0.6",
  "externally_connectable": {
    "matches": [
      "*://mycolor.today/*",
      "http://lh.dev:3000/*",
      "http://localhost:3000/*"
    ]
  }
}
